<?xml version="1.0"?>
<odoo>
	
		<record id="form_one_seq" model="ir.sequence">
			<field name="name">Acta Form One</field>
			<field name="code">form.one</field>
			<field name="prefix">Acta Nº </field>
			<field name="padding">5</field>
			<field name="active">TRUE</field>
			<field eval="1" name="number_next"/>
			<field eval="1" name="number_increment"/>
			<field name="implementation">standard</field>
		</record>


		<record id="form_final_work_seq" model="ir.sequence">
			<field name="name">Acta Form Final Work</field>
			<field name="code">form.final_work</field>
			<field name="prefix">OBRA Nº </field>
			<field name="padding">5</field>
			<field name="active">TRUE</field>
			<field eval="1" name="number_next"/>
			<field eval="1" name="number_increment"/>
			<field name="implementation">standard</field>
		</record>

</odoo>