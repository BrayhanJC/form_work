<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="form_commitment_view_form" model="ir.ui.view">
		<field name="name">form.commitment.form</field>
		<field name="model">form.commitment</field>
		<field name="arch" type="xml">

			

			<form>

				<group col="4">
					<field name="name"/>
					<field name="task_commitment"/>
				</group>

				<group col="4">
					<field name="date_current"/>

				</group>

			</form>

		</field>
	</record>


	<record id="form_commitment_view_tree" model="ir.ui.view">
		<field name="name">form.commitment.tree</field>
		<field name="model">form.commitment</field>
		<field name="arch" type="xml">

			<tree>
				<field name="task_commitment"/>
				<field name="name"/>
				<field name="date_current"/>
			</tree>

		</field>
	</record>

