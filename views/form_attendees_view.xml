<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="form_attendees_view_form" model="ir.ui.view">
		<field name="name">form.attendees.form</field>
		<field name="model">form.attendees</field>
		<field name="arch" type="xml">

			

			<form>

				<group col="4">
					<field name="name"/>
					<field name="company_id"/>
				</group>

				<group col="4">
					<field name="hour_begin"/>
					<field name="phone_attendees"/>
				</group>

				<group col="4">
					<field name="email_attendees"/>
				</group>
			</form>

		</field>
	</record>


	<record id="form_attendees_view_tree" model="ir.ui.view">
		<field name="name">form.attendees.tree</field>
		<field name="model">form.attendees</field>
		<field name="arch" type="xml">

			<tree>
				<field name="name"/>
				<field name="company_id"/>
				<field name="hour_begin"/>
				<field name="phone_attendees"/>
				<field name="email_attendees"/>
			</tree>

		</field>
	</record>

